<?import javafx.scene.layout.VBox?>

<BorderPane maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MapController">
   <center>
      <GoogleMapView fx:id="mapView" prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER" />
   </center>
   <top>
      <VBox prefWidth="600.0" BorderPane.alignment="CENTER">
         <children>
            <GridPane vgap="10.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="60.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="146.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="143.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="152.0" />
                  <ColumnConstraints hgrow="SOMETIMES" maxWidth="295.0" minWidth="10.0" prefWidth="61.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Button fx:id="mapBackButton" mnemonicParsing="false" onAction="#onBackPressed" text="Back" GridPane.halignment="CENTER" />
                  <Button fx:id="addReportButton" mnemonicParsing="false" onAction="#onAddReportPressed" text="Add a Water Report" GridPane.columnIndex="1" GridPane.halignment="CENTER" />
                   <Button fx:id="addQualityReportButton" mnemonicParsing="false" onAction="#onAddQualityReportPressed" text="Add a Quality Report" GridPane.columnIndex="2" GridPane.halignment="CENTER" />
                  <Button fx:id="clearButton" mnemonicParsing="false" onAction="#onClearPressed" text="Clear" GridPane.columnIndex="4" GridPane.halignment="CENTER" />
                  <Label alignment="CENTER_RIGHT" contentDisplay="RIGHT" text="You selected: " GridPane.columnIndex="1" GridPane.halignment="RIGHT" GridPane.rowIndex="1" />
                  <Label fx:id="selectedPin" text="Label" GridPane.columnIndex="2" GridPane.rowIndex="1" />
                  <Button fx:id="viewHistoricalReportButton" alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#onHistoricalReportPressed" text="View Historical Report" textAlignment="CENTER" GridPane.columnIndex="3" GridPane.halignment="CENTER" />
               </children>
               <padding>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </padding>
               <VBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </VBox.margin>
            </GridPane>
            <Separator prefWidth="200.0" />
         </children>
      </VBox>
   </top>
